<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context="com.gentlehealthcare.mobilecare.intravenousnew.IntravOrdersListAct">

    <include
        android:id="@+id/in_intrav_title"
        layout="@layout/layout_title_intrav"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:layout_alignParentTop="true"></include>


    <include
        android:id="@+id/in_intrav_select"
        layout="@layout/layout_intrav_tab"
        android:layout_width="fill_parent"
        android:layout_height="60dp"
        android:layout_below="@id/in_intrav_title"></include>


    <FrameLayout
        android:id="@+id/fl_container"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@id/in_intrav_select"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp" />


</RelativeLayout>
